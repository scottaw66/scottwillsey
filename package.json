{
  "name": "scottwillsey3.0",
  "version": "5.0.0",
  "description": "Astro 3 version of ScottWillsey.com",
  "scripts": {
    "dev": "export NODE_ENV=development && astro dev",
    "dev:h": "export NODE_ENV=development && astro dev --host",
    "start": "npm run dev",
    "start:h": "npm run dev:h",
    "build": "export NODE_ENV=production && astro build",
    "build:p": "npm run build && npm run preview",
    "build:ph": "npm run build && npm run preview:h",
    "preview": "astro preview",
    "preview:h": "astro preview --host"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scottaw66/scottwillsey3.0.git"
  },
  "keywords": [
    "astro",
    "blog",
    "site",
    "scottwillsey"
  ],
  "author": "Scott Willsey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scottaw66/scottwillsey3.0/issues"
  },
  "homepage": "https://github.com/scottaw66/scottwillsey3.0#readme",
  "dependencies": {
    "@astrojs/rss": "^4.0.6",
    "@astrojs/sitemap": "^3.1.6",
    "astro": "^4.11.0",
    "astro-expressive-code": "^0.35.3",
    "astro-font": "^0.0.81",
    "astro-pagefind": "https://gitpkg.now.sh/shishkin/astro-pagefind/packages/astro-pagefind?main",
    "date-fns": "^3.6.0",
    "markdown-it": "^14.1.0",
    "normalize.css": "^8.0.1",
    "rehype-accessible-emojis": "^0.3.2",
    "remark-toc": "^9.0.0",
    "sanitize-html": "^2.13.0"
  },
  "devDependencies": {
    "eslint": "^9.5.0",
    "eslint-plugin-astro": "^1.2.2",
    "prettier": "^3.3.2",
    "prettier-plugin-astro": "^0.14.0"
  }
}
